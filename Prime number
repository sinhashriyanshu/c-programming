#include<stdio.h>
int main()
{
 int n,i;
 printf("Enter A Number");
 scanf("%d",&n);
    if(n==1)
     printf("%d is not a prime number",n);
  for(i=2;i<n;i++)
  {
      if(n%i==0)
      {
        printf("%d is not a prime number",n);
        break;
  }
  }
  
  if(i==n)
  
    printf("%d is a prime",n);
}


